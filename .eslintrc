// 20160424125431
// https://raw.githubusercontent.com/bbc/morph-module-generator/master/generators/app/templates/eslintrc?token=AFlEQjCVIZ1AnPzI4sjTnwttsaJLcCTDks5XJe52wA%3D%3D

{
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "jsx": true
  },
  "env": {
    "node": true,
    "browser": true,
    "jasmine": true
  },
  "rules": {
    "array-bracket-spacing": 2,
    "block-scoped-var": 2,
    "block-spacing": 2,
    "brace-style": 2,
    "camelcase": 2,
    "comma-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "comma-style": 2,
    "complexity": [
      1,
      10
    ],
    "computed-property-spacing": [
      2,
      "never"
    ],
    "consistent-return": 2,
    "curly": 2,
    "eol-last": 2,
    "eqeqeq": 2,
    "indent": 2,
    "jsx-quotes": 1,
    "key-spacing": [
      2,
      {
        "beforeColon": false,
        "afterColon": true,
        "mode": "minimum"
      }
    ],
    "max-depth": [
      1,
      4
    ],
    "max-nested-callbacks": [
      1,
      8
    ],
    "max-params": [
      2,
      4
    ],
    "max-statements": [
      1,
      20
    ],
    "no-else-return": 1,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extra-bind": 2,
    "no-implied-eval": 2,
    "no-inline-comments": 1,
    "no-lonely-if": 1,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-magic-numbers": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": 2,
    "no-multi-spaces": 1,
    "no-multi-str": 1,
    "no-nested-ternary": 2,
    "no-param-reassign": 2,
    "no-plusplus": 1,
    "no-return-assign": 2,
    "no-self-compare": 2,
    "no-sequences": 1,
    "no-spaced-func": 2,
    "no-trailing-spaces": 2,
    "no-useless-call": 2,
    "no-use-before-define": 2,
    "no-unexpected-multiline": 1,
    "no-unneeded-ternary": 2,
    "no-warning-comments": [
      1,
      {
        "terms": [
          "todo",
          "fixme"
        ],
        "location": "start"
      }
    ],
    "semi": [
      2,
      "always"
    ],
    "semi-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "keyword-spacing": 2,
    "space-before-blocks": 2,
    "space-before-function-paren": [
      2,
      "never"
    ],
    "space-in-parens": [
      2,
      "never"
    ],
    "space-infix-ops": 2,
    "space-unary-ops": 2,
    "spaced-comment": [
      2,
      "always",
      {
        "exceptions": [
          "-",
          "+"
        ]
      }
    ],
    "vars-on-top": 1,
    "yoda": 1
  }
}
